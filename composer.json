{
    "name": "aloisogomes/laravel-db-tenant",
    "description": "Library to help you have multiple connections to the database while keeping the context clean without changing anything in the structure of your tables",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aloiso Gomes",
            "email": "aloiso.gomes@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "AloisoGomes\\LaravelDbTenant\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AloisoGomes\\LaravelDbTenant\\TenantServiceProvider"
            ],
            "aliases": {
                "Tenant": "AloisoGomes\\LaravelDbTenant\\Facades\\Tenant"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10.8",
        "phpunit/phpunit": "^12.4"
    }
}
